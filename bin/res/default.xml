<ResourceDictionary Author="汪荣(QQ:386666951)">
    <ImageBrush x:Key="BorderMask" Source="images/default/mask.png" />
    <ImageBrush x:Key="mainbkgnd" Source="images/default/bkg.jpg" ViewCorner="1278,798,0,0"  OpacityMask="#FFFFFF" Stretch="Fill" />

    <ImageBrush x:Key="bkgnd" Source="images/default/bg.bmp" Opacity="1" ViewCorner="8" />
    <ImageBrush x:Key="textbox_bk" Source="images/default/Edit.png" Opacity="1" ViewCorner="4" Stretch="Fill" />

    <ImageBrush x:Key="sousuo_n" Source="images/default/main_search_menu.png" Opacity="1" />
    <ImageBrush x:Key="sousuo_h" Source="images/default/main_search_menu.png" Opacity="0.8" />

    <ImageBrush x:Key="tooltip" Source="images/default/urltip_background.png" Opacity="1" ViewCorner="4" />

    <ImageBrush x:Key="tabbkgnd" Source="images/default/tab_ctrl_bkground.png" Opacity="1" ViewCorner="0" />
    <ImageBrush x:Key="menubkgnd" Source="images/default/menubg.png" ViewCorner="8" />

    <ImageBrush x:Key="list_m" Source="images/default/list-m.bmp" ViewCorner="1" />
    <ImageBrush x:Key="list_n" Source="images/default/list-n.bmp" ViewCorner="1" />
    <ImageBrush x:Key="list_d" Source="images/default/list-d.bmp" ViewCorner="1" />

    <ImageBrush x:Key="list_bk" Source="images/default/list-bk.bmp" ViewCorner="1" />
    <ImageBrush x:Key="split_line" Source="images/default/split_line.png" Stretch="Uniform" />
    

    <ImageBrush x:Key="normal_tabitem" Source="images/default/TabButton1.png" ViewBox="0,0,100,35" ViewCorner="4" />
    <ImageBrush x:Key="hot_tabitem" Source="images/default/TabButton1.png" ViewBox="0,105,100,140" ViewCorner="4" />
    <ImageBrush x:Key="press_tabitem" Source="images/default/TabButton1.png" ViewBox="0,35,100,70" ViewCorner="4" />

    <SolidColorBrush x:Key="Gradient01" Color="#EEEEEEEE" Opacity="1" />
    <SolidColorBrush x:Key="Gradient02" Color="#EEFFCCEE" Opacity="1" />
    <SolidColorBrush x:Key="Gradient03" Color="#EEFFCCEE" Opacity="1" />

    <LinearGradientBrush x:Key="Gradient01" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Color="#EEEEEEEE" Offset="0.0" />
        <GradientStop Color="#EE99EE" Offset="0.25" />
        <GradientStop Color="#99EECC" Offset="0.75" />
        <GradientStop Color="#DDFFCCEE" Offset="1.0" />
    </LinearGradientBrush>

    <!-- RadioButton资源定义 -->
    <ImageBrush x:Key="Radio.Unchecked.n" Source="images/default/Radio.png" ViewBox="0,0,16,16" />
    <ImageBrush x:Key="Radio.Unchecked.h" Source="images/default/Radio.png" ViewBox="16,0,32,16" />
    <ImageBrush x:Key="Radio.Unchecked.p" Source="images/default/Radio.png" ViewBox="32,0,48,16" />
    <ImageBrush x:Key="Radio.Unchecked.d" Source="images/default/Radio.png" ViewBox="48,0,60,16" />

    <ImageBrush x:Key="Radio.Checked.n" Source="images/default/Radio.png" ViewBox="64,0,80,16" />
    <ImageBrush x:Key="Radio.Checked.h" Source="images/default/Radio.png" ViewBox="80,0,96,16" />
    <ImageBrush x:Key="Radio.Checked.p" Source="images/default/Radio.png" ViewBox="96,0,112,16" />
    <ImageBrush x:Key="Radio.Checked.d" Source="images/default/Radio.png" ViewBox="112,0,128,16" />

    <!-- CheckButton资源定义 -->
    <ImageBrush x:Key="Check.Unchecked.n" Source="images/default/checkbox.png" ViewBox="0,0,15,15" />
    <ImageBrush x:Key="Check.Unchecked.h" Source="images/default/checkbox.png" ViewBox="15,0,30,15" />
    <ImageBrush x:Key="Check.Unchecked.p" Source="images/default/checkbox.png" ViewBox="30,0,45,15" />
    <ImageBrush x:Key="Check.Unchecked.d" Source="images/default/checkbox.png" ViewBox="45,0,60,15" />

    <ImageBrush x:Key="Check.Checked.n" Source="images/default/checkbox.png" ViewBox="60,0,75,15" />
    <ImageBrush x:Key="Check.Checked.h" Source="images/default/checkbox.png" ViewBox="75,0,90,15" />
    <ImageBrush x:Key="Check.Checked.p" Source="images/default/checkbox.png" ViewBox="90,0,105,15" />
    <ImageBrush x:Key="Check.Checked.d" Source="images/default/checkbox.png" ViewBox="105,0,120,15" />

    <!-- 垂直滚动条资源定义 -->
    <ImageBrush x:Key="vscroll" Source="images/default/scrollbar.png" ViewBox="0,16,16,32" />
