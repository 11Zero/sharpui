    <ImageBrush x:Key="vscroll.track.thumb.h" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="18,126,34,142" />
    <ImageBrush x:Key="vscroll.track.thumb.p" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="36,126,52,142" />

    <ImageBrush x:Key="vscroll.track.page.n" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="0,161,16,177" />
    <ImageBrush x:Key="vscroll.track.page.h" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="0,161,16,177" />
    <ImageBrush x:Key="vscroll.track.page.p" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="0,161,16,177" />

    <ImageBrush x:Key="vscroll.toparrow" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,90,16,106" />
    <ImageBrush x:Key="vscroll.bottomarrow" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,108,16,124" />
    <ImageBrush x:Key="vscrollbar.bk" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,161,16,177" />

    <ImageBrush x:Key="vscroll.track.glyph" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,144,16,160" />

    <!-- 水平滚动条资源定义 -->
    <ImageBrush x:Key="hscroll.track.thumb.n" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="0,36,16,52" />
    <ImageBrush x:Key="hscroll.track.thumb.h" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="18,36,34,52" />
    <ImageBrush x:Key="hscroll.track.thumb.p" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="36,36,52,52" />

    <ImageBrush x:Key="hscroll.track.page.n" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="0,72,16,88" />
    <ImageBrush x:Key="hscroll.track.page.h" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="0,72,16,88" />
    <ImageBrush x:Key="hscroll.track.page.p" Source="images/default/scrollbar.png" ViewCorner="2" ViewBox="0,72,16,88" />

    <ImageBrush x:Key="hscroll.leftarrow" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,0,16,16" />
    <ImageBrush x:Key="hscroll.rightarrow" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,18,16,34" />
    <ImageBrush x:Key="hscroll.bk" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,72,16,88" />
    <ImageBrush x:Key="hscroll.track.glyph" Source="images/default/scrollbar.png" ViewCorner="4" ViewBox="0,54,16,70" />

    <!-- 滑块条资源定义 -->
    <ImageBrush x:Key="slider_bk" Source="images/default/Slider_bk.bmp" OpacityMask="#000000" ViewCorner="0,0,4,0" Stretch="UniformToFill" />
    <ImageBrush x:Key="slider_drag" Source="images/default/SliderDrag.png" ViewBox="0" ViewCorner="0" Stretch="Uniform" />
    
    <!-- TreeView扩展和收缩按钮资源定义 -->
    <ImageBrush x:Key="Expanded" Source="images/default/tree_plus.gif" Stretch="Uniform" />
    <ImageBrush x:Key="Collapsed" Source="images/default/tree_minus.gif" Stretch="Uniform" />

    <ImageBrush x:Key="save_res" Source="images/default/save.png" ViewBox="0" ViewCorner="0" Stretch="Uniform" />

    <!-- ProgressBar资源定义 -->
    <ImageBrush x:Key="progress_background" Source="images/default/progress_background_icon.png" ViewCorner="4,2,4,2" />
    <ImageBrush x:Key="progress_foreground" Source="images/default/progress_foreground_icon.png" ViewCorner="4,2,4,2" Stretch="Fill" />

    <!-- 组合框资源定义 -->
    <ImageBrush x:Key="normal_combo" Source="images/default/combo.png" ViewBox="0,0,100,22" ViewCorner="2,2,24,2" />
    <ImageBrush x:Key="hot_combo" Source="images/default/combo.png" ViewBox="0,22,100,44" ViewCorner="2,2,24,2" />
    <ImageBrush x:Key="press_combo" Source="images/default/combo.png" ViewBox="0,44,100,66" ViewCorner="2,2,24,2" />

    <!-- Button资源定义 -->
    <ImageBrush x:Key="normal_btn" Source="images/default/expand.button.normal.png" OpacityMask="#EFF0F1" ViewCorner="4" />
    <ImageBrush x:Key="hot_btn" Source="images/default/button.png" ViewCorner="4" OpacityMask="#FFFFFFFF" ViewBox="0,0,82,26" />
    <ImageBrush x:Key="down_btn" Source="images/default/button.png" ViewCorner="4" ViewBox="0,26,82,52" />
    <ImageBrush x:Key="focus_btn" Source="images/default/button.png" ViewCorner="4" ViewBox="0,52,82,78" />

    <!-- SystemCloseButton资源定义 -->
    <ImageBrush x:Key="normal_close" Source="images/common/btn_close_normal.png" ViewCorner="0" />
    <ImageBrush x:Key="hot_close" Source="images/common/btn_close_highlight.png" ViewCorner="0" />
    <ImageBrush x:Key="down_close" Source="images/common/btn_close_down.png" ViewCorner="0" />

    <!-- SystemMaxButton资源定义 -->
    <ImageBrush x:Key="normal_max" Source="images/common/btn_max_normal.png" ViewCorner="0" />
    <ImageBrush x:Key="hot_max" Source="images/common/btn_max_highlight.png" ViewCorner="0" />
    <ImageBrush x:Key="down_max" Source="images/common/btn_max_down.png" ViewCorner="0" />

    <!-- SystemRestoreButton资源定义 -->
    <ImageBrush x:Key="normal_restore" Source="images/common/btn_restore_normal.png" ViewCorner="0" />
    <ImageBrush x:Key="hot_restore" Source="images/common/btn_restore_highlight.png" ViewCorner="0" />
    <ImageBrush x:Key="down_restore" Source="images/common/btn_restore_down.png" ViewCorner="0" />

    <!-- SystemMinButton资源定义 -->
    <ImageBrush x:Key="normal_min" Source="images/common/btn_mini_normal.png" ViewCorner="0" />
    <ImageBrush x:Key="hot_min" Source="images/common/btn_mini_highlight.png" ViewCorner="0" />
    <ImageBrush x:Key="down_min" Source="images/common/btn_mini_down.png" ViewCorner="0" />
    <ImageBrush x:Key="down_arrow" Source="images/common/scrollbar-h.png" ViewCorner="0" ViewBox="0,32,16,48"/>

    <!-- 字体资源定义 -->
    <Font x:Key="boxfont" Family="宋体" Size="8" Weight="0" Italic="0" Underline="0"/>
    <Font x:Key="font1" Family="宋体" Size="18" Weight="1" Italic="1" Underline="0"/>
    <Font x:Key="ftooltip" Family="宋体" Size="16" Weight="0" Italic="0" Underline="0"/>
    <Font x:Key="fttitle" Family="宋体" Size="22" Weight="1" Italic="0" Underline="0"/>
    <Font x:Key="tabfocusfont" Family="宋体" Size="16" Weight="1" Italic="0" Underline="0"/>
    <Font x:Key="fpanel" Family="宋体" Size="22" Weight="1" Italic="0" Underline="0"/>
    <Font x:Key="fresitem" Family="宋体" Size="17" Weight="1" Italic="0" Underline="0"/>
    <Font x:Key="loginfont" Family="宋体" Size="14" Weight="1" Italic="0" Underline="0"/>
    <Font x:Key="fontedittitle" Family="宋体" Size="16" Weight="1" Italic="0" Underline="0"/>

    <Cursor x:Key="splitter_ver" Source="images/common/splitter_ver.cur" />
    <Cursor x:Key="splitter_hor" Source="images/common/splitter_hor.cur" />

    <!-- Window控件样式定义 -->
    <Style TargetType="Window">
        <Setter Property="Background" Value="{mainbkgnd}" />
        <Setter Property="Background1" Value="#668888" />
        <Setter Property="BorderThickness" Value="1,1,1,2"/>
        <Setter Property="RoundSize" Value="2" />
        <Style.Triggers>
            <Trigger Property="IsInactivate" Value="True">
                <Setter Property="Background" Value="{mainbkgnd}" />
                <Setter Property="BorderThickness" Value="1,1,1,2"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- ToolTip控件样式定义 -->
    <Style TargetType="ToolTip">
        <Setter Property="Font1" Value="{ftooltip}" />
        <Setter Property="Background" Value="{tooltip}" />
        <Setter Property="Foreground" Value="#666666"/>
        <Setter Property="Padding" Value="4,4,4,6" />
    </Style>

    <!-- TabItem控件样式定义 -->
    <Style TargetType="TabItem">
        <Setter Property="Background" Value="{normal_tabitem}" />
        <Setter Property="BorderBrush" Value="#888888"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{hot_tabitem}" />
                <Setter Property="Foreground" Value="#00FFFF" />
                <Setter Property="FontSize" Value="16" />
                <Setter Property="Opacity" Value="0.8" />
                <Setter Property="BorderBrush" Value="#CCECF0" />
                <Setter Property="BorderThickness" Value="0" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{press_tabitem}" />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Background" Value="{press_tabitem}" />
                <Setter Property="Opacity" Value="0.8" />
                <Setter Property="Font" Value="{tabfocusfont}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- ListBox控件样式定义 -->
    <Style TargetType="ListBox">
        <Setter Property="Background" Value="#FFFFFF" />
        <Setter Property="BorderBrush" Value="#8888FF"/>
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="2" />
    </Style>

    <!-- ComboBox控件样式定义 -->
    <Style TargetType="ComboBox">
        <Setter Property="Background" Value="{normal_combo}" />
        <Setter Property="Foreground" Value="#666666"/>
        <Setter Property="Padding" Value="4,0,4,0" />
        <Setter Property="DropDownHeight" Value="50" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{hot_combo}" />
                <Setter Property="Foreground" Value="#666666"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{press_combo}" />
                <Setter Property="Foreground" Value="#666666"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="ListView" BasedOn="{x:Type ListBox}" />

    <!-- ComboBox.TextBox控件样式定义 -->
    <Style TargetType="ComboBox.TextBox">
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="BorderBrush" Value="{x:Null}" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{x:Null}" />
                <Setter Property="BorderBrush" Value="{x:Null}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- ComboBox.ListBox控件样式定义 -->
    <Style TargetType="ComboBox.ListBox">
        <Setter Property="Background" Value="#FFFFFF" />
        <Setter Property="BorderBrush" Value="#888888"/>
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="0" />
        <Style.Triggers>
        </Style.Triggers>
    </Style>

    <!-- ComboBoxItem控件样式定义 -->
    <Style TargetType="ComboBoxItem" >
        <Setter Property="Background" Value="#FFFFFF" />
        <Setter Property="BorderBrush" Value="#888888"/>
        <Setter Property="Foreground" Value="#000000"/>
        <Setter Property="Padding" Value="2" />
        <Style.Triggers>
            <Trigger Property="IsFocused" Value="True" BasedOn="">
                <Setter Property="Background" Value="#1FFF99" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- Slider控件样式定义 -->
    <Style TargetType="Slider">
        <Setter Property="Background" Value="{slider_bk}" />
    </Style>

    <Style TargetType="Slider.Thumb">
        <Setter Property="Background" Value="{slider_drag}" />
    </Style>

    <!-- TextBlock控件样式定义 -->
    <Style TargetType="TextBlock">
        <Setter Property="Foreground" Value="#FF000000" />
        <Setter Property="Background1" Value="#FF0000FF" />
    </Style>

    <!-- TextBox控件样式定义 -->
    <Style TargetType="TextBox">
        <Setter Property="Background" Value="{textbox_bk}" />
        <Setter Property="BorderBrush" Value="#888888"/>
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="2" />
        <Style.Triggers>
        </Style.Triggers>
    </Style>

    <!-- PasswordBox控件样式定义 -->
    <Style TargetType="PasswordBox" BasedOn="{x:Type TextBox}" />

    <!-- TreeViewItem控件样式定义 -->
    <Style TargetType="TreeViewItem">
        <Style.Triggers>
            <Trigger Property="IsFocused" Value="True" BasedOn="">
                <Setter Property="Background" Value="#FF6666FF" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- ListBoxItem控件样式定义 -->
    <Style TargetType="ListBoxItem">
        <Setter Property="Background1" Value="#661FFF99" />
        <Setter Property="Background1" Value="{x:Null}" />
        <Setter Property="BorderBrush" Value="#88888888"/>
        <Setter Property="Foreground" Value="#FF0088"/>
        <Setter Property="BorderThickness" Value="0,0,0,1" />
        <Setter Property="Padding" Value="2" />
        <Style.Triggers>
            <Trigger Property="IsFocused" Value="True" BasedOn="">
                <Setter Property="Background" Value="#88FFFF11" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- ListViewItem控件样式定义 -->
    <Style TargetType="ListViewItem" BasedOn="{x:Type ListBoxItem}" />

    <Style x:Key="TitleMenu" >
        <Setter Property="Background" Value="{normal_menu}" />
        <Setter Property="Margin" Value="0" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{hot_menu}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{down_menu}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- ListViewHeader控件样式定义 -->
    <Style TargetType="ListViewHeader" >
        <Setter Property="Background" Value="{list_bk}" />
        <Setter Property="Height" Value="20" />
    </Style>

    <!-- ColomnHeader控件样式定义 -->
    <Style TargetType="GridViewColumnHeader">
        <Setter Property="Background" Value="{list_n}" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{list_m}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{list_d}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- GridHeaderGripper控件样式定义 -->
    <Style TargetType="GridHeaderGripper" >
        <Setter Property="Background" Value="{split_line}" />
        <Setter Property="Width" Value="4" />
        <Setter Property="Cursor" Value="{splitter_ver}" />
    </Style>

    <Style TargetType="Menu" >
        <Setter Property="BorderThickness" Value="3" />
        <Setter Property="Padding" Value="4" />
        <Setter Property="Background" Value="{menubkgnd}" />
    </Style>
    
    <Style TargetType="MenuItem">
        <Setter Property="Background" Value="#FFFFFF1" />
        <Setter Property="BorderBrush" Value="#888888"/>
        <Setter Property="Foreground" Value="#000000"/>
        <Setter Property="Padding" Value="5,2,5,2" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#CCFF11" />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Background" Value="#FFFF11" />
            </Trigger>
            <Trigger Property="IsDisabled" Value="True">
                <Setter Property="Foreground" Value="#BBBBBB" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="TreeView">
        <Setter Property="Background" Value="#ccEEEEEE" />
        <Setter Property="Foreground" Value="#000000" />
        <Setter Property="BorderBrush" Value="#CC8888FF"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Expanded" Value="{Expanded}"/>
        <Setter Property="Collapsed" Value="{Collapsed}"/>
    </Style>

    <Style TargetType="Button">
        <Setter Property="Background" Value="{normal_btn}" />
        <Setter Property="BorderThickness" Value="0"/>
        <Style.Triggers x:Key="Test">
            <Trigger Property="IsMouseOver" Value="True" >
                <Setter Property="Background" Value="{hot_btn}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{down_btn}" />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Background" Value="{focus_btn}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="Sousuo">
        <Setter Property="Background" Value="{sousuo_n}" />
        <Style.Triggers x:Key="Test">
            <Trigger Property="IsMouseOver" Value="True" >
                <Setter Property="Background" Value="{sousuo_h}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="SystemCloseButton" >
        <Setter Property="Background" Value="{normal_close}" />
        <Setter Property="Margin" Value="0,0,2,0" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{hot_close}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{down_close}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="SystemMinButton">
        <Setter Property="Background" Value="{normal_min}" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{hot_min}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{down_min}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="SystemMaxButton" >
        <Setter Property="Restore" Value="{normal_restore}" />
        <Setter Property="Max" Value="{normal_max}" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Restore" Value="{hot_restore}" />
                <Setter Property="Max" Value="{hot_max}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Restore" Value="{down_restore}" />
                <Setter Property="Max" Value="{down_max}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="ProgressBar" >
        <Setter Property="Background" Value="{progress_background}" />
        <Setter Property="Thumb" Value="{progress_foreground}" />
    </Style>

    <Style TargetType="RadioButton">
        <Setter Property="Checked" Value="{Radio.Checked.n}" />
        <Setter Property="Unchecked" Value="{Radio.Unchecked.n}" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Checked" Value="{Radio.Checked.h}" />
                <Setter Property="Unchecked" Value="{Radio.Unchecked.h}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Checked" Value="{Radio.Checked.p}" />
                <Setter Property="Unchecked" Value="{Radio.Unchecked.p}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="CheckButton">
        <Setter Property="Checked" Value="{Check.Checked.n}" />
        <Setter Property="Unchecked" Value="{Check.Unchecked.n}" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Checked" Value="{Check.Checked.h}" />
                <Setter Property="Unchecked" Value="{Check.Unchecked.h}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Checked" Value="{Check.Checked.p}" />
                <Setter Property="Unchecked" Value="{Check.Unchecked.p}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="VScrollBar" >
